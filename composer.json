{
  "name": "webappid/laravel-airline",
  "description": "Laravel airline package",
  "license": "MIT",
  "authors": [
    {
      "name": "Dyan Galih",
      "email": "dyan.galih@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "WebAppId\\Airline\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebAppId\\Airline\\Tests\\": "src/tests"
    }
  },
  "require": {
    "php" : ">=7.2.0",
    "illuminate/support": "^6.0|^7.0",
    "doctrine/dbal": "^2.5",
    "webappid/laravel-user": "^2.0",
    "webappid/laravel-country": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "squizlabs/php_codesniffer": "^3.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^4.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "WebAppId\\Airline\\ServiceProvider"
      ]
    }
  }
}
